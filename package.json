{
    "name": "react-t9n",
    "version": "5.0.0-9",
    "description": "React-style library for handling translation",
    "repository": "git@github.com:globuslimited/react-t9n",
    "author": "Andrei izotov <faradaytrs@gmail.com>",
    "license": "MIT",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "tsc",
        "release": "set -a && source .env && set +a && release-it",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "peerDependencies": {
        "ramda": "^0.29.0",
        "react": "^18.2"
    },
    "devDependencies": {
        "@testing-library/react-hooks": "8.0.1",
        "@types/ramda": "0.29.10",
        "@types/react": "18.2.48",
        "@vitest/coverage-c8": "0.33.0",
        "c8": "9.1.0",
        "prettier": "^3.0.3",
        "ramda": "0.29.1",
        "react": "18.2.0",
        "react-test-renderer": "18.2.0",
        "release-it": "17.0.3",
        "typescript": "5.3.3",
        "vitest": "1.2.1"
    },
    "release-it": {
        "git": {
            "commit": true,
            "tag": true,
            "push": true,
            "requireBranch": "main"
        },
        "github": {
            "release": true
        },
        "npm": {
            "publish": false
        }
    },
    "prettier": {
        "printWidth": 120,
        "trailingComma": "all",
        "tabWidth": 4,
        "singleQuote": false,
        "bracketSpacing": false,
        "useTabs": false,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid",
        "semi": true,
        "quoteProps": "as-needed"
    },
    "exports": {
        ".": {
            "react-server": "./dist/shared/index.js",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    }
}
